{
  "name": "survos/liform",
  "description": "Library to transform Symfony Forms into Json Schema",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nacho Mart√≠n",
      "email": "nacho@limenius.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Limenius\\Liform\\": "src/Limenius/Liform"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Limenius\\Liform\\Tests\\": "tests/Limenius/Liform/Tests"
    }
  },
  "require": {
    "php": "^8.2",
    "symfony/form": "^6.4|^7.1|^8.0",
    "symfony/translation": "^6.4|^7.1|^8.0",
    "symfony/serializer": "^6.4|^7.1|^8.0",
    "symfony/validator": "^6.4|^7.1|^8.0",
    "symfony/translation-contracts": "^1.0|^2.1|^3.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "escapestudios/symfony2-coding-standard": "^3.0",
    "phpunit/phpunit": "^11.4",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpstan/phpstan": "^1.8",
    "rector/rector": "^1.2"
  },
  "scripts": {
    "test": "phpunit",
    "cs": "phpcs"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
